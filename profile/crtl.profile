        set obfuscate "true";
        set cleanup "true";

        # SpawnTo
        set spawnto_x86 "c:\\windows\\syswow64\\cmd.exe";
        set spawnto_x64 "c:\\program files (x86)\\microsoft\\edge\\application\\msedge.exe";

        # SMB Named Pipes Names
